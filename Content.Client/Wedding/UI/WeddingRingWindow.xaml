<DefaultWindow xmlns="https://spacestation14.io"
            MinSize="300 300"
            SetSize="300 300"
            Title="{Loc 'wedding-ring-window-title'}">
    <BoxContainer Orientation="Vertical" SeparationOverride="4" MinWidth="150">
        <Label Name="SelectedCoupleLabel"
               Text="{Loc 'wedding-ring-selected-couple'}">
        </Label>
        <ItemList Name="CoupleList"
                  SizeFlagsStretchRatio="8"
                  VerticalExpand="True">
        </ItemList>
    </BoxContainer>
</DefaultWindow>
